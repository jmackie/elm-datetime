digraph G {
    node [shape=oval, width=1.5, height=1.5];
    ranksep=0.6;
    nodesep=0.4;

    m1 [shape=point,width=0.01,height=0.01];
    {Zone,Posix} -> m1 [dir=none];
    m1 -> DateTime;
    DateTime -> Date;
    DateTime -> Time;
    Date -> Year;
    Date -> Month;
    Date -> Day;
    Time -> Hour;
    Time -> Minute;
    Time -> Second;
    Time -> Millisecond;
}
